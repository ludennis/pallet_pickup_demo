 <root main_tree_to_execute = "AFL_BT" >
   <BehaviorTree ID="AFL_BT">
    <Sequence name="PickupPallet">
      <ForkActionNode
        name="DropFork"
        TargetHeight="0.025"/>
      <WaitForPalletDetection
        name="WaitForPalletDetection"
        PalletTfName="pallet"
        MapTfName="map"
        PalletPose="{DetectedPalletPose}"/>
      <ForkActionNode
        name="RaiseForkToPalletHeight"
        PalletPose="{DetectedPalletPose}"
        PalletThickness="0.12"
        PalletBottomPadding="0.01"/>
      <MoveActionNode
        name="ApproachPallet"
        TargetPose="{DetectedPalletPose}"
        TargetPoseOffset="0.2"/>
      <MoveActionNode
        name="InsertForkThroughPallet"
        TargetPose="{DetectedPalletPose}"
        TargetPoseOffset="-0.9"/>
      <ForkActionNode
        name="PickupPallet"
        TargetHeightOffset="0.25"/>
      <MoveActionNode
        name="BackAway"
        TargetPose="{DetectedPalletPose}"
        TargetPoseOffset="0.2"/>
      <ForkActionNode
        name="LowerForkCloseToGround"
        TargetHeight="0.1"/>
    </Sequence>
   </BehaviorTree>
 </root>
