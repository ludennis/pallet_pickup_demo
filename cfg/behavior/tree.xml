 <root main_tree_to_execute = "AFL_BT" >
   <BehaviorTree ID="AFL_BT">
    <Sequence name="root_sequence">
      <WaitForPalletDetection
        name="WaitForPalletDetection"
        PalletTfName="pallet"
        RobotTfName="back_footprint"
        PalletPose="{DetectedPalletPose}"/>
      <ForkActionNode
        name="RaiseForkActionNode"
        PalletPose="{DetectedPalletPose}"
        PalletThickness="120"
        PalletBottomPadding="10"/>
      <MoveActionNode
        name="MoveActionNode"
        TargetPose="{DetectedPalletPose}"/>
      <ForkActionNode
        name="RaiseForkActionNodeByOffset"
        TargetHeightOffset="100"/>
      <MoveActionNode
        name="BackAwayFromPallet"
        TargetPose="{DetectedPalletPose}"
        BackAwayDistance="1500"/>
      <ForkActionNode
        name="LowerForkCloseToGround"
        TargetHeight="100"/>
    </Sequence>
   </BehaviorTree>
 </root>
